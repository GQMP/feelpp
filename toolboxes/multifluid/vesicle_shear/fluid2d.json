{
    "Name": "Square2d",
    "ShortName":"Square2d",
    "Model": "Stokes",
    "Parameters":
    {
        "gammaDot": { "value":"2" },
	"yMax": { "value":"4." },
	"yMin": { "value":"-4." }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                "UpperLeft":
                {
                    "expr":"{gammaDot*y,0}:gammaDot:y"
                },
                "LowerLeft":
                {
                    "expr":"{gammaDot*y,0}:gammaDot:y"
                },
                "UpperRight":
                {
                    "expr":"{gammaDot*y,0}:gammaDot:y"
                },
                "LowerRight":
                {
                    "expr":"{gammaDot*y,0}:gammaDot:y"
                },
                "Top":
                {
                    "expr":"{gammaDot*yMax,0}:gammaDot:yMax"
                },
                "Bottom":
                {
                    "expr":"{gammaDot*yMin,0}:gammaDot:yMin"
                }
            }
        }
    },
    "PostProcess":
    {
        "Fields":["velocity","pressure","density", "viscosity", "pid"]
    }

}
