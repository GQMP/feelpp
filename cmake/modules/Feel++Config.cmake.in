if (FEELPP_HAS_PARAVIEW)
  set(FEELPP_PARAVIEW_DIR "@FEELPP_PARAVIEW_DIR@")
elseif (FEELPP_HAS_VTK)
  set(FEELPP_VTK_DIR "@FEELPP_VTK_DIR@")
